{
  "name": "react-search-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "jest-enzyme": "^7.1.2",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.11.0",
    "react-app-polyfill": "^1.0.4",
    "react-dom": "^16.11.0",
    "react-scripts": "3.2.0",
    "react-test-renderer": "^16.11.0"
  },
  "scripts": {
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && npm run build-css -- -w -r",
    "build-app": "react-scripts build",
    "start-app": "react-scripts start",
    "build": "npm-run-all -p build-css build-app",
    "start": "npm-run-all -p watch-css start-app",
    "test": "react-scripts test --coverage --verbose --watchAll=false",
    "test:dev": "react-scripts test --verbose",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^3.1.1"
  }
}
